<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <CalendarView
        android:id="@+id/calendar1"
        android:layout_width="294dp"
        android:layout_height="269dp"
        android:maxDate="31/12/2022"
        android:minDate="04/22/2022"
        android:showWeekNumber ="false"
        android:selectedWeekVertcicalBr="@color/black"
        android:weekSeparationLineColor="#0f0"
        android:unfocusedMonthDateColor="#00f"
        android:weekNumberColor="#f00"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

    <CalendarView
        android:id="@+id/calendar2"
        android:layout_width="288dp"
        android:layout_height="265dp"
        android:maxDate="31/12/2022"
        android:minDate="04/22/2022"
        android:showWeekNumber ="false"
        android:selectedWeekVertcicalBr="@color/black"
        android:weekSeparationLineColor="#0f0"
        android:unfocusedMonthDateColor="#00f"
        android:weekNumberColor="#f00"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="305dp" />

    <TextView
        android:id="@+id/text1"
        android:layout_width="81dp"
        android:layout_height="31dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="280dp"
        tools:layout_editor_absoluteY="657dp" />

    <ProgressBar
        android:id="@+id/progressBar"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="222dp"
        android:layout_height="44dp"
        tools:layout_editor_absoluteX="28dp"
        tools:layout_editor_absoluteY="582dp" />

    <SeekBar
        android:id="@+id/seekBar"
        android:layout_width="222dp"
        android:layout_height="44dp"
        android:max="2"
        android:min="1"
        tools:layout_editor_absoluteX="28dp"
        tools:layout_editor_absoluteY="644dp" />
</androidx.constraintlayout.widget.ConstraintLayout>